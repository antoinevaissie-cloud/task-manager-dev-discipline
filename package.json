{
  "name": "task-manager-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "concurrently \"npm --workspace server run dev\" \"npm --workspace web run dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
